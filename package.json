{
  "name": "xtrixui-cli",
  "version": "1.0.10",
  "type": "module",
  "description": "A CLI tool for scaffolding XtrixUI projects with ease.",
  "bin": {
    "xtrixui": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "start": "node ./dist/index.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "author": "Kingshuk Mondal",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/ImKKingshuk/XtrixUI-CLI.git"
  },
  "bugs": {
    "url": "https://github.com/ImKKingshuk/XtrixUI-CLI/issues"
  },
  "homepage": "https://github.com/ImKKingshuk/XtrixUI-CLI#readme",
  "keywords": [
    "cli",
    "xtrixui-cli",
    "xtrixui"
  ],
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@rslib/core": "^0.5.4",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.10",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "execa": "^9.5.2",
    "figlet": "^1.8.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.5.0",
    "prompts": "^2.4.2"
  }
}
